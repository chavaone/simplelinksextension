{
  "manifest_version": 2,

  "name": "Url List Extension",
  "description": "This extension is a simple list of links.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png"
  },

  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "activeTab"
  ],

  "background": {
        "scripts": ["js/link.js", "extension.js"],
        "persistent": true
    }
}
